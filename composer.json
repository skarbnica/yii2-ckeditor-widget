{
    "name": "skarbnica/yii2-ckeditor-widget",
    "description": "custom ckeditor",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","ckeditor"],
    "license": "MIT",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Alexander Tarasyuk",
            "email": "djeager404@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "skarbnica/css-text": "^0.1.1"
    },
    "autoload": {
        "psr-4": {
            "skarbnica\\ckeditor\\src\\": ""
        }
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm"
            }
        }
    },
    "repositories": [
        {
          "type":"git",
          "name":"skarbnica/css-text",
          "url": "https://github.com/skarbnica/css-text"
        }
  ]
}
